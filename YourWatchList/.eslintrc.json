{
    "env": {
        "node": true,
        "browser": true,
        "es6": true
    },
    "parser": "babel-eslint",
    "extends": "airbnb",
    "rules": {
        "linebreak-style": 0,
        "react/prefer-stateless-function": 0,
        "react/jsx-filename-extension": 0,
        "arrow-body-style": 0,
        "class-methods-use-this": 0,
        "global-require": 0
    }
}